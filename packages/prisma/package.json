{
    "name": "@flux/prisma",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npx tsc",
        "generate": "run-script-os",
        "generate:windows": "doppler run npx prisma generate && copy src\\index.d.ts dist /y",
        "generate:macos": "doppler run npx prisma generate && cp src/index.d.ts dist/index.d.ts",
        "seed": "doppler run npx prisma db seed",
        "reset": "npm run generate && npm run seed",
        "studio": "doppler run npx prisma studio"
    },
    "prisma": {
        "seed": "ts-node src/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^4.8.0",
        "prisma": "^4.8.0"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "run-script-os": "^1.1.6",
        "typescript": "^4.9.4"
    },
    "bundledDependencies": [
        "@prisma/client",
        ".prisma"
    ]
}
