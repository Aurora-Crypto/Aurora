{
    "get": {
        "schema": {
            "description": "Gets user transactions based on supplied user id.",
            "tags": ["user"],
            "summary": "Get user transactions.",
            "params": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string",
                        "description": "user id"
                    }
                }
            },
            "response": {
                "200": {
                    "description": "OK",
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "date": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "receivedQuantity": {
                                "type": "number"
                            },
                            "receivedCurrency": {
                                "type": "string"
                            },
                            "sentQuantity": {
                                "type": "number"
                            },
                            "sentCurrency": {
                                "type": "string"
                            },
                            "feeAmount": {
                                "type": "number"
                            },
                            "feeCurrency": {
                                "type": "string"
                            },
                            "tag": {
                                "type": "string",
                                "enum": [
                                    "GIFT",
                                    "LOSS",
                                    "DONATION",
                                    "MARGIN_FEE",
                                    "FORK",
                                    "AIRDROP",
                                    "MINED",
                                    "PAYMENT",
                                    "STAKED",
                                    "MARGIN",
                                    "MARGIN_REBATE",
                                    "INTEREST",
                                    "INCOME"
                                ]
                            },
                            "user": {
                                "type": "string"
                            }
                        }
                    }
                },
                "404": {
                    "description": "Not found. Missing user id or transaction id parameter.",
                    "type": "object",
                    "properties": {
                        "code": { "type": "number" },
                        "message": { "type": "string" }
                    }
                },
                "5XX": {
                    "description": "Unexpected error.",
                    "type": "object",
                    "properties": {
                        "code": { "type": "number" },
                        "message": { "type": "string" }
                    }
                }
            }
        }
    },
    "post": {
        "schema": {
            "description": "Creates a transaction based on supplied transaction details and links to provided user id.",
