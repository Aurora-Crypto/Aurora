                "properties": {
                    "userId": {
                        "type": "string",
                        "description": "user id"
                    }
                }
            },
            "body": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "date": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "receivedQuantity": {
                        "type": "number"
                    },
                    "receivedCurrency": {
                        "type": "string"
                    },
                    "sentQuantity": {
                        "type": "number"
                    },
                    "sentCurrency": {
                        "type": "string"
                    },
                    "feeAmount": {
                        "type": "number"
                    },
                    "feeCurrency": {
                        "type": "string"
                    },
                    "tag": {
                        "type": "string",
                        "enum": [
                            "GIFT",
                            "LOSS",
                            "DONATION",
                            "MARGIN_FEE",
                            "FORK",
                            "AIRDROP",
                            "MINED",
                            "PAYMENT",
                            "STAKED",
                            "MARGIN",
                            "MARGIN_REBATE",
                            "INTEREST",
                            "INCOME"
                        ]
                    }
                }
            },
            "response": {
                "201": {
                    "description": "Successful creation and linking.",
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "date": {
                            "type": "string",
                            "format": "date-time"
                        },
                        "receivedQuantity": {
                            "type": "number"
                        },
                        "receivedCurrency": {
                            "type": "string"
                        },
                        "sentQuantity": {
                            "type": "number"
                        },
                        "sentCurrency": {
                            "type": "string"
                        },
                        "feeAmount": {
                            "type": "number"
                        },
                        "feeCurrency": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string",
                            "enum": [
                                "GIFT",
                                "LOSS",
                                "DONATION",
                                "MARGIN_FEE",
                                "FORK",
                                "AIRDROP",
                                "MINED",
                                "PAYMENT",
                                "STAKED",
                                "MARGIN",
                                "MARGIN_REBATE",
                                "INTEREST",
                                "INCOME"
                            ]
                        },
                        "user": {
                            "type": "string"
                        }
                    }
                },
                "404": {
                    "description": "Not found. Missing user id or transaction parameter.",
                    "type": "object",
                    "properties": {
                        "code": { "type": "number" },
                        "message": { "type": "string" }
                    }
                },
                "5XX": {
                    "description": "Unexpected error.",
                    "type": "object",
                    "properties": {
                        "code": { "type": "number" },
                        "message": { "type": "string" }
                    }
                }
            }
        }
    },
    "delete": {
        "schema": {
            "description": "Deletes (unlinks) a wallet based on supplied user id and wallet address.",
            "tags": ["user"],
            "summary": "Deletes user wallet.",
            "params": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string",
                        "description": "user id"
                    }
                }
            },
            "body": {
                "type": "object",
                "properties": {
                    "walletAddress": { "type": "string" }
                }
            },
            "response": {
                "204": {
                    "description": "Successful deletion and unlinking.",
                    "type": "null"
                },
                "404": {
                    "description": "Not found. Missing user id or wallet address parameter.",
                    "type": "object",
                    "properties": {
                        "code": { "type": "number" },
                        "message": { "type": "string" }
                    }
                },
                "5XX": {
                    "description": "Unexpected error.",
                    "type": "object",
                    "properties": {
                        "code": { "type": "number" },
                        "message": { "type": "string" }
                    }
                }
            }
        }
    }
}
